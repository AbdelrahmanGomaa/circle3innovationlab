    <section class="breadcrumb-area pt-50px pb-50px bg-white pattern-bg">
      <div class="container">
        <div class="col-lg-8 me-auto">
          <div class="breadcrumb-content">
            <ul
              class="generic-list-item generic-list-item-arrow d-flex flex-wrap align-items-center"
            >
              <li><a href="index.html">Home</a></li>
              <li><a href="#">Development</a></li>
              <li><a href="#">Java</a></li>
            </ul>
            <div class="section-heading">
              <h2 class="section__title">
                 @Model.Value("courseName")
              </h2>
              <p class="section__desc pt-2 lh-30">
                 @Model.Value("courseSubDesc")
              </p>
            </div>
            <!-- end section-heading -->
            <div class="d-flex flex-wrap align-items-center pt-3">
                
                 @if (Model.HasValue("courseTags")){
                      
                    
                        var tags = Model.Value<IEnumerable<string>>("courseTags");
                        var index = 0;
                    
                    
                    @foreach (var tag in tags)
                    {
                        if (index % 2 == 0)
                        {
                            <div class="course-badge">@tag</div>
                        }
                        else
                        {
                            <div class="course-badge blue">@tag</div>
                        }
                        index++;
                    }  
                        
                    }
                  
<!--              <h6 class="ribbon ribbon-lg me-2 bg-3 text-white">Bestseller</h6>
-->              
              
            </div>
            <!-- end d-flex -->
                          <div class="rating-wrap d-flex flex-wrap align-items-center">
                <div class="review-stars">
                  @if (Model.HasValue("courseDescountNote")){
    
                        <span class="rating-number">@Model.Value("courseDescountNote")</span>
                    }
                </div>
                
              </div>

                <p class="card-price text-black font-weight-bold">
                    @Model.Value("coursePriceTitle")
                    @Model.Value("coursePrice")
                    @if (Model.HasValue("courseOldPrice")){
                    <span class="before-price font-weight-medium">@Model.Value("courseOldPrice")</span>
                  }
                 </p>
            
         
